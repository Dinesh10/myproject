    <body>
    <?php
 /*
   && (and get)
   || (or get)
   !  (not get)
  */
    $en=60;
    $ma=58;
    $sc=80;
    $so=70;
    $ta=99;
    
    echo "Total : ".($en+$ma+$sc+$so+$ta);
    echo "<br>Average : S".($en+$ma+$sc+$so+$ta)/5;
    
    
    if($en>=35&&$ma>=35&&$sc>=35&&$so>=35&&$ta>=35)
    {
    	echo "</br>Result : All Pass";
    }
    else 
    {
    	echo  "Result : Fail";
    }
	?>
    </body>

</html>