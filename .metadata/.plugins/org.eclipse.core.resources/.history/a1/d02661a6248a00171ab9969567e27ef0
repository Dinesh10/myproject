    <body>
    <?php
 /*
   && (and get)
   || (or get)
   !  (not get)
  */
    $en=100;
    $ma=35;
    $sc=80;
    $so=70;
    $ta=99;
    
    echo "Total : ".($en+$ma+$sc+$so+$ta);
    echo "<br>Average :".($en+$ma+$sc+$so+$ta)/5;
    
    
    if($en>=35&&$ma>=35&&$sc>=35&&$so>=35&&$ta>=35)
    	if($en<=100&&$ma<=100&&$sc<=100&&$so<=100&&$ta<=100)
    		
    {
    	echo "</br>Result : All Pass";
    }
    else 
    {
    	echo  "Result : Fail";
    }
	?>
    </body>

